flags:
  rewrite: false

cert:
  issuer: letsencrypt-prod-homelab
  secretName: le-issuer-account-key-grafana

service:
  name: grafana
  subdomain: grafana
  port:
    number: 80

annotations:
  hajimari.io/enable: "true"
  hajimari.io/appName: "grafana"

ingress:
  name: grafana
  subdomain: grafana
  routePath: /

hosts:
%{ for host in hosts ~}
- name: ${host}
%{ endfor ~}
