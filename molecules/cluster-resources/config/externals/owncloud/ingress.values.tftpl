flags:
  rewrite: false

cert:
  issuer: letsencrypt-prod-homelab
  secretName: le-issuer-account-key-owncloud

service:
  name: owncloud
  port:
    number: 80

annotations:
  hajimari.io/enable: "true"
  hajimari.io/appName: "owncloud"

ingress:
  name: owncloud
  subdomain: owncloud
  routePath: /

hosts:
%{ for host in hosts ~}
- name: ${host}
%{ endfor ~}
