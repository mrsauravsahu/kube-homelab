flags:
  rewrite: false

cert:
  issuer: letsencrypt-prod-homelab
  secretName: le-issuer-account-key-minio

service:
  name: minio-console
  port:
    number: 9001

annotations:
  hajimari.io/enable: "true"
  hajimari.io/appName: "minio"

ingress:
  name: minio
  subdomain: minio
  routePath: /

hosts:
%{ for host in hosts ~}
- name: ${host}
%{ endfor ~}
