flags:
  rewrite: false

cert:
  issuer: letsencrypt-prod-homelab
  secretName: le-issuer-account-key-nocodb

service:
  name: nocodb
  port:
    number: 8080

annotations:
  hajimari.io/enable: "true"
  hajimari.io/appName: "nocodb"

ingress:
  name: nocodb
  subdomain: nocodb
  routePath: /

hosts:
%{ for host in hosts ~}
- name: ${host}
%{ endfor ~}
